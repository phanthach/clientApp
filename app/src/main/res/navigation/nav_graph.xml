<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentHome">
    <fragment
        android:id="@+id/fragmentHome"
        android:name="com.example.clientapp.Presentation.UI.Home.FragmentHome"
        android:label="FragmentHome"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_fragmentHome_to_fragmentScanQR"
            app:destination="@id/fragmentScanQR" />
        <action
            android:id="@+id/action_fragmentHome_to_fragmentHistory"
            app:destination="@id/fragmentHistory" />
        <action
            android:id="@+id/action_fragmentHome_to_fragmentAccount"
            app:destination="@id/fragmentAccount" />
    </fragment>
    <fragment
        android:id="@+id/fragmentHistory"
        android:name="com.example.clientapp.Presentation.UI.History.FragmentHistory"
        android:label="FragmentHistory"
        tools:layout="@layout/fragment_history"/>
    <fragment
        android:id="@+id/fragmentScanQR"
        android:name="com.example.clientapp.Presentation.UI.ScanQR.FragmentScanQR"
        android:label="FragmentScanQR"
        tools:layout="@layout/fragment_scanqr"/>
    <fragment
        android:id="@+id/fragmentAccount"
        android:name="com.example.clientapp.Presentation.UI.Account.FragmentAccount"
        android:label="FragmentAccount"
        tools:layout="@layout/fragment_account"/>
</navigation>